<%@ page import="com.blockchain.timebank.entity.OrderEntity" %>
<%@ page import="java.util.List" %>
<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=0">
    <title>WeUI</title>
    <link rel="stylesheet" href="../css/weui.css">
    <link rel="stylesheet" href="../css/weui-example.css">
    <link rel="stylesheet" href="../css/bootstrap.min.css">
</head>
<body>
<!-- 使用 -->
<div class="weui-tab">
    <div class="weui-tab__panel">
        <!--界面中的内容-->
        <div class="weui-tab">
            <div class="weui-navbar">
                <div class="weui-navbar__item weui-bar__item_on">
                    所有服务
                </div>
                <div class="weui-navbar__item">
                    已完成
                </div>
                <div class="weui-navbar__item">
                    未完成
                </div>
            </div>
            <div class="weui-tab__panel">
                <div class="weui-panel__bd">
                    <%
                        List<OrderEntity> list = (List<OrderEntity>) request.getAttribute("list");
                        for (OrderEntity orderEntity : list) {
                    %>
                    <a href="javascript:void(0);" class="weui-media-box weui-media-box_appmsg">
                        <div class="weui-media-box__hd">
                            <img class="weui-media-box__thumb" src="data:image/png;base64," alt="">
                        </div>
                        <div class="weui-media-box__bd">
                            <h4 class="weui-media-box__title">申请者ID：<%out.print(orderEntity.getApplyUserId());%></h4>
                            <p class="weui-media-box__desc">服务者ID：<%out.print(orderEntity.getServiceUserId());%></p>
                            <p class="weui-media-box__desc">支付方式：<%out.print(orderEntity.getPayWay());%></p>
                            <p class="weui-media-box__desc">订单状态：<%out.print(orderEntity.getStatus());%></p>
                        </div>
                    </a>

                    <%}%>


                </div>
                <div>已完成</div>
                <div class="weui-panel__bd">
                    <a href="javascript:void(0);" class="weui-media-box weui-media-box_appmsg">
                        <div class="weui-media-box__hd">
                            <img class="weui-media-box__thumb" src="data:image/png;base64," alt="">
                        </div>
                        <div class="weui-media-box__bd">
                            <h4 class="weui-media-box__title">服务名称</h4>
                            <p class="weui-media-box__desc">服务类别</p>
                            <p class="weui-media-box__desc">服务者</p>

                            <p class="weui-media-box__desc">服务详情由各种物质组成的巨型球状天体，叫做星球。</p>

                        </div>
                    </a>
                    <a href="javascript:void(0);" class="weui-media-box weui-media-box_appmsg">
                        <div class="weui-media-box__hd">
                            <img class="weui-media-box__thumb" src="data:image/png;base64," alt="">
                        </div>
                        <div class="weui-media-box__bd">
                            <h4 class="weui-media-box__title">服务名称</h4>
                            <p class="weui-media-box__desc">服务类别</p>
                            <p class="weui-media-box__desc">服务者</p>
                            <p class="weui-media-box__desc">服务详情由各种物质组成的巨型球状天体，叫做星球。星球有一定的形状，有自己的运行轨道。</p>
                        </div>
                    </a>
                </div>
                <div>未完成</div>
                <div class="weui-panel__bd">
                    <a href="javascript:void(0);" class="weui-media-box weui-media-box_appmsg">
                        <div class="weui-media-box__hd">
                            <img class="weui-media-box__thumb" src="data:image/png;base64," alt="">
                        </div>
                        <div class="weui-media-box__bd">
                            <h4 class="weui-media-box__title">服务名称</h4>
                            <p class="weui-media-box__desc">服务类别</p>
                            <p class="weui-media-box__desc">服务者</p>
                            <p class="weui-media-box__desc">服务详情由各种物质组成的巨型球状天体，叫做星球。</p>

                        </div>
                    </a>
                </div>
            </div>
        </div>
    </div>
    <div class="weui-tabbar">
        <a href="javascript:;" class="weui-tabbar__item ">
            <img src="../img/首页.png" alt="" class="weui-tabbar__icon">
            <p class="weui-tabbar__label">首页</p>
        </a>
        <a href="javascript:;" class="weui-tabbar__item">
            <img src="../img/服务.png" alt="" class="weui-tabbar__icon">
            <p class="weui-tabbar__label">服务</p>
        </a>
        <a href="javascript:;" class="weui-tabbar__item weui-bar__item_on">
            <img src="../img/我的.png" alt="" class="weui-tabbar__icon">
            <p class="weui-tabbar__label">我的</p>
        </a>

    </div>
</div>
</body>
</html>